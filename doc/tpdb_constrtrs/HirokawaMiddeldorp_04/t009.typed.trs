        ,b3
        ,i) -> idle(B()
                   ,closed()
                   ,up()
                   ,false()
                   ,true()
                   ,b3
                   ,i)
    busy(B()
        ,closed()
        ,stop()
        ,false()
        ,false()
        ,true()
        ,i) -> idle(B()
                   ,closed()
                   ,up()
                   ,false()
                   ,false()
