(STRATEGY
    INNERMOST)

(VAR
    X X1 X2 Y Z)
(DATATYPES
    A = ÂµX.< mark(X), 0, ok(X) >)
(SIGNATURES
    active :: [A] -> A
    from :: [A] -> A
    cons :: [A x A] -> A
    s :: [A] -> A
    sel :: [A x A] -> A
    proper :: [A] -> A
    top :: [A] -> A)
(RULES
    active(from(X)) -> mark(cons(X
                                ,from(s(X))))
    active(sel(0(),cons(X,Y))) ->
      mark(X)
    active(sel(s(X),cons(Y,Z))) ->
      mark(sel(X,Z))
    active(from(X)) ->
      from(active(X))
    active(cons(X1,X2)) ->
      cons(active(X1),X2)
    active(s(X)) -> s(active(X))
    active(sel(X1,X2)) ->
      sel(active(X1),X2)
    active(sel(X1,X2)) -> sel(X1
                             ,active(X2))
    from(mark(X)) -> mark(from(X))
    cons(mark(X1),X2) ->
      mark(cons(X1,X2))
    s(mark(X)) -> mark(s(X))
    sel(mark(X1),X2) -> mark(sel(X1
                                ,X2))
    sel(X1,mark(X2)) -> mark(sel(X1
                                ,X2))
    proper(from(X)) ->
      from(proper(X))
    proper(cons(X1,X2)) ->
      cons(proper(X1),proper(X2))
    proper(s(X)) -> s(proper(X))
    proper(sel(X1,X2)) ->
      sel(proper(X1),proper(X2))
    proper(0()) -> ok(0())
    from(ok(X)) -> ok(from(X))
    cons(ok(X1),ok(X2)) ->
      ok(cons(X1,X2))
    s(ok(X)) -> ok(s(X))
    sel(ok(X1),ok(X2)) -> ok(sel(X1
                                ,X2))
    top(mark(X)) -> top(proper(X))
    top(ok(X)) -> top(active(X)))
