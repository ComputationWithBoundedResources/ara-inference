(STRATEGY
    INNERMOST)

(VAR
    x x' xs y)
(DATATYPES
    A = ÂµX.< Cons(X, X), Nil, True, False >)
(SIGNATURES
    add0 :: [A x A] -> A
    notEmpty :: [A] -> A
    goal :: [A x A] -> A)
(RULES
    add0(x',Cons(x,xs)) ->
      add0(Cons(Cons(Nil(),Nil()),x')
