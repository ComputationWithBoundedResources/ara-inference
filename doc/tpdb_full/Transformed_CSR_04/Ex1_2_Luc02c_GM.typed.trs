(STRATEGY
    INNERMOST)

(VAR
    X X1 X2 Y Z)
(DATATYPES
    A = ÂµX.< cons(X, X), from(X), s(X), 2nd(X) >)
(SIGNATURES
    a__2nd :: [A] -> A
    a__from :: [A] -> A
    mark :: [A] -> A)
(RULES
    a__2nd(cons(X,cons(Y,Z))) ->
      mark(Y)
    a__from(X) -> cons(mark(X)
