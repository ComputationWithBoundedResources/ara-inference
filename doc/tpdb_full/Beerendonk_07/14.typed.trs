(STRATEGY
    INNERMOST)

(VAR
    x y)
(DATATYPES
    A = ÂµX.< s(X), true, false, 0 >)
(SIGNATURES
    cond1 :: [A x A] -> A
    cond2 :: [A x A x A] -> A
    gr :: [A x A] -> A
    neq :: [A x A] -> A
    p :: [A] -> A)
(RULES
    cond1(s(x),y) -> cond2(gr(s(x)
                             ,y)
                          ,s(x)
                          ,y)
    cond2(true(),x,y) -> cond1(y,y)
    cond2(false(),x,y) -> cond1(p(x)
                               ,y)
    gr(0(),x) -> false()
    gr(s(x),0()) -> true()
    gr(s(x),s(y)) -> gr(x,y)
    neq(0(),0()) -> false()
    neq(0(),s(x)) -> true()
    neq(s(x),0()) -> true()
    neq(s(x),s(y)) -> neq(x,y)
    p(0()) -> 0()
    p(s(x)) -> x)
