(VAR x xs y ys)


(RULES

  add0#(Cons(x,xs),y) -> c_1(add0#(xs,Cons(S(),y)))

  mul0#(Cons(x,xs),y) ->= add0#(mul0(xs,y),y)
  mul0#(Cons(x,xs),y) ->= mul0#(xs,y)

  add0(Cons(x,xs),y)  ->= add0(xs,Cons(S(),y))
  add0(Nil(),y)       ->= y

  mul0(Cons(x,xs),y)  ->= add0(mul0(xs,y),y)
  mul0(Nil(),y)       ->= Nil()

)


(COMMENT

  add0(Cons(x,xs),y) -> add0(xs,Cons(S(),y))
  add0(Nil(),y) -> y
  goal(xs,ys) -> mul0(xs,ys)
  mul0(Cons(x,xs),y) -> add0(mul0(xs,y),y)
  mul0(Nil(),y) -> Nil()


)
