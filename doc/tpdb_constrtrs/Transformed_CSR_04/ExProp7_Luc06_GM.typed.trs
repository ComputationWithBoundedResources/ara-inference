(STRATEGY
    INNERMOST)

(VAR
    X X1 X2)
(DATATYPES
    A = ÂµX.< 0, cons(X, X), f(X), s(X), p(X) >)
(SIGNATURES
    a__f :: [A] -> A
    a__p :: [A] -> A
    mark :: [A] -> A)
(RULES
    a__f(0()) -> cons(0(),f(s(0())))
    a__f(s(0())) ->
      a__f(a__p(s(0())))
    a__p(s(X)) -> mark(X)
