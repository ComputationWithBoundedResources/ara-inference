(RULES
   @(nil, ys) -> ys
   @(dd(x, xs), ys) -> dd(x, @(xs, ys))
   rev(nil) -> nil
   rev(dd(x, xs)) -> @(rev(xs), dd(x, nil))
   shuffle(nil) -> nil
   shuffle(dd(x, xs)) -> dd(x, shuffle(rev(xs)))
   shuffleshuffle(nil) -> nil
   shuffleshuffle(dd(x,xs)) -> dd(x, shuffleshuffle(shuffle(xs)))
)
