(STRATEGY
    INNERMOST)

(VAR
    x xs)
(DATATYPES
    A = ÂµX.< Cons(X, X), Nil, True, isEmpty[Match](X), False >)
(SIGNATURES
    list :: [A] -> A
    notEmpty :: [A] -> A
    goal :: [A] -> A)
(RULES
    list(Cons(x,xs)) -> list(xs)
