(STRATEGY
    INNERMOST)

(VAR
    L X)
(DATATYPES
    A = ÂµX.< nil, cons(X, X), s(X), 0 >)
(SIGNATURES
    incr :: [A] -> A
    adx :: [A] -> A
    nats :: [] -> A
    zeros :: [] -> A
    head :: [A] -> A
    tail :: [A] -> A)
(RULES
    incr(nil()) -> nil()
    incr(cons(X,L)) -> cons(s(X)
