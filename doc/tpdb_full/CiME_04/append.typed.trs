(STRATEGY
    INNERMOST)

(VAR
    l l1 l2 x)
(DATATYPES
    A = ÂµX.< nil, true, cons(X, X), false >)
(SIGNATURES
    is_empty :: [A] -> A
    hd :: [A] -> A
    tl :: [A] -> A
    append :: [A x A] -> A
    ifappend :: [A x A x A] -> A)
(RULES
    is_empty(nil()) -> true()
    is_empty(cons(x,l)) -> false()
    hd(cons(x,l)) -> x
    tl(cons(x,l)) -> l
    append(l1,l2) -> ifappend(l1
                             ,l2
                             ,l1)
    ifappend(l1,l2,nil()) -> l2
    ifappend(l1,l2,cons(x,l)) ->
      cons(x,append(l,l2)))
