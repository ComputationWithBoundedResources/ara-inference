(STRATEGY
    INNERMOST)

(VAR
    ALPHA BETA X)
(DATATYPES
    A = ÂµX.< one, a, zero, plus(X, X), times(X, X), minus(X, X), neg(X), div(X, X), exp(X, X), two, ln(X) >)
(SIGNATURES
    dx :: [A] -> A)
(RULES
    dx(X) -> one()
    dx(a()) -> zero()
    dx(plus(ALPHA,BETA)) ->
      plus(dx(ALPHA),dx(BETA))
    dx(times(ALPHA,BETA)) ->
      plus(times(BETA,dx(ALPHA))
          ,times(ALPHA,dx(BETA)))
    dx(minus(ALPHA,BETA)) ->
      minus(dx(ALPHA),dx(BETA))
    dx(neg(ALPHA)) -> neg(dx(ALPHA))
    dx(div(ALPHA,BETA)) ->
      minus(div(dx(ALPHA),BETA)
           ,times(ALPHA
                 ,div(dx(BETA),exp(BETA,two()))))
    dx(ln(ALPHA)) -> div(dx(ALPHA)
                        ,ALPHA)
    dx(exp(ALPHA,BETA)) ->
      plus(times(BETA
                ,times(exp(ALPHA
                          ,minus(BETA,one()))
                      ,dx(ALPHA)))
          ,times(exp(ALPHA,BETA)
                ,times(ln(ALPHA),dx(BETA)))))
