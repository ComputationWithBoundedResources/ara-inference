(STRATEGY
    INNERMOST)

(VAR
    N X X1 X2 Y Z)
(DATATYPES
    A = ÂµX.< cons(X, X), recip(X), n__terms(X), n__s(X), 0, nil, n__first(X, X) >)
(SIGNATURES
    terms :: [A] -> A
    sqr :: [A] -> A
    dbl :: [A] -> A
    add :: [A x A] -> A
    first :: [A x A] -> A
    half :: [A] -> A
    s :: [A] -> A
    activate :: [A] -> A)
(RULES
    terms(N) -> cons(recip(sqr(N))
                    ,n__terms(n__s(N)))
    sqr(0()) -> 0()
    sqr(s(X)) -> s(add(sqr(X)
                      ,dbl(X)))
    dbl(0()) -> 0()
    dbl(s(X)) -> s(s(dbl(X)))
    add(0(),X) -> X
    add(s(X),Y) -> s(add(X,Y))
    first(0(),X) -> nil()
    first(s(X),cons(Y,Z)) -> cons(Y
                                 ,n__first(X,activate(Z)))
    half(0()) -> 0()
    half(s(0())) -> 0()
    half(s(s(X))) -> s(half(X))
    half(dbl(X)) -> X
    terms(X) -> n__terms(X)
    s(X) -> n__s(X)
    first(X1,X2) -> n__first(X1,X2)
    activate(n__terms(X)) ->
      terms(activate(X))
    activate(n__s(X)) ->
      s(activate(X))
    activate(n__first(X1,X2)) ->
      first(activate(X1),activate(X2))
    activate(X) -> X)
