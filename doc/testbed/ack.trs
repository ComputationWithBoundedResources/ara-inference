(STRATEGY
    INNERMOST)

(VAR
    m n x x' xs xs')
(DATATYPES
    A = ÂµX.< Cons(X, X), Nil >)
(SIGNATURES
    ack :: [A x A] -> A
    goal :: [A x A] -> A)
(RULES
    ack(Cons(x,xs),Nil()) -> ack(xs
