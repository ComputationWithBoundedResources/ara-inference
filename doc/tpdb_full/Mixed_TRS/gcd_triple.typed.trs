                           ,z)
    gcd(x,s(y),s(z)) -> gcd(x
                           ,-(max(y,z),min(y,z))
                           ,s(min(y,z)))
    gcd(s(x),y,s(z)) -> gcd(-(max(x
                                 ,z)
                             ,min(x,z))
                           ,y
                           ,s(min(x,z)))
    gcd(x,0(),0()) -> x
    gcd(0(),y,0()) -> y
    gcd(0(),0(),z) -> z)
