(STRATEGY
    INNERMOST)

(VAR
    c x y)
(DATATYPES
    A = ÂµX.< 0, s(X), true, false, times(X, X) >)
(SIGNATURES
    lt :: [A x A] -> A
    fac :: [A] -> A
    help :: [A x A] -> A
    if :: [A x A x A] -> A)
(RULES
    lt(0(),s(x)) -> true()
    lt(x,0()) -> false()
