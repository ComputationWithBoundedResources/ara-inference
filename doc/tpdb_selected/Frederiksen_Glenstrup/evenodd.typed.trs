(STRATEGY
    INNERMOST)

(VAR
    x xs)
(DATATYPES
    A = ÂµX.< Cons(X, X), Nil, False, True >)
(SIGNATURES
    odd :: [A] -> A
    even :: [A] -> A
    notEmpty :: [A] -> A
    evenodd :: [A] -> A)
(RULES
    odd(Cons(x,xs)) -> even(xs)
    odd(Nil()) -> False()
    even(Cons(x,xs)) -> odd(xs)
    notEmpty(Cons(x,xs)) -> True()
    notEmpty(Nil()) -> False()
    even(Nil()) -> True()
    evenodd(x) -> even(x))
