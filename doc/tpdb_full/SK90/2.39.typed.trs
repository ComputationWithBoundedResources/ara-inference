(STRATEGY
    INNERMOST)

(VAR
    x y z)
(DATATYPES
    A = ÂµX.< nil, .(X, X), true, false >)
(SIGNATURES
    rev :: [A] -> A
    car :: [A] -> A
    cdr :: [A] -> A
    null :: [A] -> A
    ++ :: [A x A] -> A)
(RULES
    rev(nil()) -> nil()
    rev(.(x,y)) -> ++(rev(y)
