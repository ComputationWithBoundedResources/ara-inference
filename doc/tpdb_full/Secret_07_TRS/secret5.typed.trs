(STRATEGY
    INNERMOST)

(VAR
    N X X1 X2 Y Z)
(DATATYPES
    A = ÂµX.< cs(X, X), r(X), nt(X), ns(X), 0, nil, nf(X, X) >)
(SIGNATURES
    t :: [A] -> A
    q :: [A] -> A
    d :: [A] -> A
    p :: [A x A] -> A
    f :: [A x A] -> A
