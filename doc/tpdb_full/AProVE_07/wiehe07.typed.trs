(STRATEGY
    INNERMOST)

(VAR
    x y z)
(DATATYPES
    A = ÂµX.< s(X), 0, zero, true, false >)
(SIGNATURES
    times :: [A x A] -> A
    plus :: [A x A] -> A
    id :: [A] -> A
    if :: [A x A x A] -> A
    not :: [A] -> A
    gt :: [A x A] -> A)
(RULES
    times(x,plus(y,s(z))) ->
      plus(times(x
                ,plus(y,times(s(z),0())))
          ,times(x,s(z)))
    times(x,0()) -> 0()
    times(x,s(y)) -> plus(times(x,y)
                         ,x)
    plus(s(x),s(y)) ->
      s(s(plus(if(gt(x,y),x,y)
              ,if(not(gt(x,y)),id(x),id(y)))))
    plus(s(x),x) -> plus(if(gt(x,x)
                           ,id(x)
                           ,id(x))
                        ,s(x))
    plus(zero(),y) -> y
    plus(id(x),s(y)) -> s(plus(x
                              ,if(gt(s(y),y),y,s(y))))
    id(x) -> x
    if(true(),x,y) -> x
    if(false(),x,y) -> y
    not(x) -> if(x,false(),true())
    gt(s(x),zero()) -> true()
    gt(zero(),y) -> false()
    gt(s(x),s(y)) -> gt(x,y))
