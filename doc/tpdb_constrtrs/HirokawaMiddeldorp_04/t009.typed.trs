                   ,b3
                   ,i)
    busy(S()
        ,closed()
        ,down()
        ,b1
        ,b2
        ,true()
        ,i) -> idle(S()
                   ,closed()
                   ,stop()
                   ,b1
                   ,b2
                   ,true()
                   ,i)
    busy(B()
        ,closed()
        ,up()
        ,false()
        ,b2
        ,b3
        ,i) -> idle(BF()
                   ,closed()
                   ,up()
                   ,false()
                   ,b2
                   ,b3
                   ,i)
    busy(F()
        ,closed()
        ,up()
        ,b1
        ,false()
        ,b3
        ,i) -> idle(FS()
                   ,closed()
                   ,up()
                   ,b1
                   ,false()
                   ,b3
                   ,i)
    busy(F()
        ,closed()
        ,down()
        ,b1
        ,false()
        ,b3
        ,i) -> idle(BF()
                   ,closed()
                   ,down()
                   ,b1
                   ,false()
                   ,b3
                   ,i)
    busy(S()
        ,closed()
        ,down()
        ,b1
        ,b2
        ,false()
        ,i) -> idle(FS()
                   ,closed()
                   ,down()
                   ,b1
                   ,b2
                   ,false()
                   ,i)
    busy(BF()
        ,closed()
        ,up()
        ,b1
        ,b2
        ,b3
        ,i) -> idle(F()
                   ,closed()
                   ,up()
                   ,b1
                   ,b2
                   ,b3
                   ,i)
    busy(BF()
        ,closed()
        ,down()
        ,b1
        ,b2
        ,b3
        ,i) -> idle(B()
                   ,closed()
                   ,down()
                   ,b1
                   ,b2
