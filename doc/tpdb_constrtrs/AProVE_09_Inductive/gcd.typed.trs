(STRATEGY
    INNERMOST)

(VAR
    x y)
(DATATYPES
    A = ÂµX.< s(X), true, false, 0 >)
(SIGNATURES
    minus :: [A x A] -> A
    if :: [A x A x A] -> A
    gcd :: [A x A] -> A
    if1 :: [A x A x A] -> A
    if2 :: [A x A x A] -> A
    gt :: [A x A] -> A
    ge :: [A x A] -> A)
(RULES
    minus(s(x),y) -> if(gt(s(x),y)
                       ,x
                       ,y)
    if(true(),x,y) -> s(minus(x,y))
    if(false(),x,y) -> 0()
    gcd(x,y) -> if1(ge(x,y),x,y)
    if1(true(),x,y) -> if2(gt(y,0())
                          ,x
                          ,y)
    if1(false(),x,y) -> gcd(y,x)
    if2(true(),x,y) -> gcd(minus(x
                                ,y)
                          ,y)
    if2(false(),x,y) -> x
    gt(0(),y) -> false()
    gt(s(x),0()) -> true()
    gt(s(x),s(y)) -> gt(x,y)
    ge(x,0()) -> true()
    ge(0(),s(x)) -> false()
    ge(s(x),s(y)) -> ge(x,y))
