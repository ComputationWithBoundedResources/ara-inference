(STRATEGY
    INNERMOST)

(VAR
    x y)
(DATATYPES
    A = ÂµX.< 0, true, 1, false, s(X), c(X) >)
(SIGNATURES
    f :: [A] -> A
    if :: [A x A x A] -> A
    g :: [A x A] -> A)
(RULES
    f(0()) -> true()
    f(1()) -> false()
    f(s(x)) -> f(x)
    if(true(),x,y) -> x
    if(false(),x,y) -> y
    g(s(x),s(y)) -> if(f(x)
                      ,s(x)
                      ,s(y))
    g(x,c(y)) -> g(x,g(s(c(y)),y)))
